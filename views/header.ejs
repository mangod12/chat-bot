  <style media="screen">
    /* Chatter Bot */
    .chat-page{
      margin-top: 30px;
      background: #d1d1d1;
    }
    .chat-page-history{
      background: transparent;
      color: white;
      overflow: scroll;
      overflow-x: hidden;
      height: 400px;
    }

    .sender{
      float: left;
      padding: 0px;
      width: calc(100% - 25px);
      font-size: 0.9em;
    }
    .sender .chat-sender{
      background: #fff;
      color: #262626;
      float: left;
      display: inline-block;
      padding: 10px 15px;
      border-radius: 30px;
      max-width: 300px;
      line-height: 100%;
      word-wrap: break-word;
      letter-spacing: 0.1px;
      line-height: 1.3;
      font-size:16px;
      border-bottom-left-radius: 2px;
    }
    .receiver{
      float: right;
      padding: 0px;
      width: calc(100% - 25px);
      font-size: 0.9em;
    }
    .receiver .chat-receiver{
      background: #dcf8c6;
      color: #262626;
      float: right;
      display: inline-block;
      padding: 10px 15px;
      border-radius: 30px;
      max-width: 300px;
      line-height: 100%;
      word-wrap: break-word;
      letter-spacing: 0.1px;
      line-height: 1.3;
      font-size:16px;
      border-bottom-right-radius: 2px;
    }

    .chat-page-typing-and-sending{
      background: red;
      height: 70px;
    }
    .chat-typing{
      outline:none;
      border-radius: 30px;
      width: 100%;
      height: 30px;
    }
    .chat-sending{
      margin-top: 15px;
    }
    .load-chat{
      width:100px;
    }
    /* ./Chat Bot */
  </style>
</head>
